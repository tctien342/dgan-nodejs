{
  "name": "optimize-yolo",
  "version": "1.0.0",
  "description": "Image processing for optimize yolo detection",
  "main": "index.ts",
  "author": "saintno",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start",
    "ext": "ts"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^2.7.0",
    "matplotnode": "^0.4.2",
    "opencv4nodejs": "^5.6.0"
  }
}
